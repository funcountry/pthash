{
  "output": {
    "filePath": "pthash.md",
    "style": "markdown",
    "parsableStyle": false,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 30,
    "showLineNumbers": true,
    "copyToClipboard": true
  },
  "include": [
    "src/build_phf.cpp",
    "src/test_fastmod.cpp",
    "include/pthash.hpp",
    "include/single_phf.hpp",
    "include/partitioned_phf.hpp",
    "src/util.hpp",
    "include/utils/hasher.hpp",
    "include/utils/util.hpp",
    "include/utils/encoders.hpp",
    "include/utils/bucketers.hpp",
    "external/bits/include/elias_fano.hpp",
    "external/bits/include/bit_vector.hpp",
    "external/bits/include/compact_vector.hpp",
    "external/bits/external/essentials/include/essentials.hpp",
    "external/fastmod/include/fastmod.h",
    "README_build_phf.md",
    "CMakeLists.txt"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "build/**",
      "debug_build/**",
      "**/vendor/**",
      "**/node_modules/**",
      "**/.git/**",
      "**/*.log",
      "**/*.o",
      "**/*.a",
      "**/*.so",
      "**/*.dylib",
      "**/*.dll",
      "**/*.exe",
      "**/*.obj",
      "**/*.out",
      "**/tmp/**",
      "**/temp/**",
      "**/__pycache__/**",
      "**/*.pyc",
      "**/dist/**",
      "lib/tests/**",
      "**/*.bin",
      "**/.*cache/**"
    ]
  },
  "security": {
    "enableSecurityCheck": false
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
} 